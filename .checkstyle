<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="Rob Utils Checkstyle Rules" location="tools/checkstyle/projectCheckstyleConfig.xml" type="project" description="Checkstyle Rules for this project.">
